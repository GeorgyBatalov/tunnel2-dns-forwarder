{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Tunnel2.DnsForwarder": "Debug"
    }
  },
  "DnsForwarder": {
    "ListenIpv4": "127.0.0.1",
    "UdpPort": 15053,
    "UpstreamDnsAddress": "127.0.0.1",
    "UpstreamDnsPort": 12053
  },
  "Cache": {
    "MaxPositiveCacheTtlSeconds": 60,
    "NegativeCacheTtlSeconds": 30
  },
  "RateLimit": {
    "MaxRequestsPerIp": 1000
  }
}
